          url: './hotelDetailConter'
        })
      },
      // 详情
      details() {
        wepy.navigateTo({
          url: './hotelInformation'
        })
      },      
      // 订
      ReserveShow() {
        this.showModal = true
        this.$apply();
      },
      // 立即预定
      RightReserve() {
        wepy.navigateTo({
          url: './order'
        })
      }
