    };
    components = {
      popups: popups
    }
    computed = {};
    methods = {
      handleImg() {
        if (this.hotelImageList.length > 0) {
          let ruls = []
          this.hotelImageList.forEach(item => {
            ruls.push(item.imageUrl)
          });
          wx.previewImage({
            current: this.hotelImageList[0],
            urls: ruls
          })
        }
      },
      handleSelected(index) {
        this.roomRateList[index].selected = !this.roomRateList[index].selected
        this.$apply()
      },
      handleHotelTag(index) {
        let item = this.hotelTagsList[index]
        if (item.name === 'bedType' && item.roomType === '1') {
          for (var i = 0; i < this.hotelTagsList.length; i++) {
            if (this.hotelTagsList[i]['name'] === 'bedType' && this.hotelTagsList[i].roomType === '2') {
              this.hotelTagsList[i]['checked'] = false
            }
          }
        }
        if (item.name === 'bedType' && item.roomType === '2') {
          for (var i = 0; i < this.hotelTagsList.length; i++) {
            if (this.hotelTagsList[i]['name'] === 'bedType' && this.hotelTagsList[i].roomType === '1') {
              this.hotelTagsList[i]['checked'] = false
            }
          }
        }
        if (item.checked) {
          if (this.filterRooms[item.name] !== undefined) {
            this.filterRooms[item.name] = "";
          }
          item.checked = false;
        } else {
          if (this.filterRooms[item.name] !== undefined) {
            this.filterRooms[item.name] =
              item.name === "bedType" ? item.type : 'true';
          }
          item.checked = true;
        }
        this.$apply()
        // this.planKeyId = item.id;
      },
      // 关闭预定弹窗
      closePop() {
        this.showModal = false
        this.$apply()
      },
      // 地图
      seeMap() {
        wepy.navigateTo({
          url: './map'
        })
      },
      // 评论
      comment() {
